<% form_object = form_object(@data_list, can: { wrap_all: false, wrap_id: true, label: false, placeholder: true }, css: { wrap_input: 'two wide field' }) %>

var template = '<%= j(render partial: 'item_form', locals: { f: form_object }) %>'
$('#parameters_<%= params[:index].to_i - 1 %>').after(template)

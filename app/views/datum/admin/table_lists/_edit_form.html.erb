<%= form_with model: @table_list, url: data_list_table_list_path, locale: true do |f| %>
  <% @table_list.parameters.each do |k, v| %>
    <div class="field is-horizontal">
      <label class="six wide field"><%= k.to_s.titleize %></label>
      <div class="six wide field">
        <%= text_field_tag "table_list[parameters][#{k}]", v, type: RailsData.config.mapping[@data_list.parameters[k].to_sym][:input] %>
      </div>
    </div>
  <% end %>
  <%= f.submit %>
<% end %>

<div class="ui modal" id="modal">
  <i class="fas fa-times close"></i>
  <div class="ui header">
    <%= t('.new') %>
  </div>
  
  <div class="content">
    <%= form_with model: @table_list, url: data_list_table_lists_path do |f| %>
      <% @data_list.parameters.each do |k, v| %>
        <div class="inline fields">
          <label class="six wide field"><%= k.to_s.titleize %></label>
          <div class="six wide field">
            <%= text_field_tag :"table_list[parameters][#{k}]", nil, type: RailsData.config.mapping[v.to_sym][:input] %>
          </div>
        </div>
      <% end %>
      <%= f.submit %>
    <% end %>
  </div>
</div>

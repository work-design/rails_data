<%= form_with theme: 'modal', model: @table_list, url: { action: 'update' } do |f| %>
  <%= render 'modal_header' %>
  <section class="modal-card-body">
    <%= render partial: 'com/extra/extra_form', locals: { taxon: @table_list.data_list, f: f } %>
  </section>
  <footer class="modal-card-foot">
    <%= f.submit %>
  </footer>
<% end %>
